{
  "name": "photonglass",
  "private": true,
  "version": "1.0.0",
  "description": "A network monitoring and diagnostic tool with Hyperglass-inspired UI",
  "type": "module",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch server/index.ts",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:client": "tsc && vite build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outfile=dist/server/index.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "start": "NODE_ENV=production node --experimental-specifier-resolution=node --enable-source-maps dist/server/index.js",
    "serve": "npm run build && npm run start",
    "docker:build": "docker build -t photonglass .",
    "docker:run": "docker run -p 8080:8080 -v $(pwd)/../config:/app/config photonglass"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-context-menu": "^2.1.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.3",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^4.29.19",
    "class-variance-authority": "^0.6.1",
    "clsx": "^1.2.1",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "framer-motion": "^10.12.18",
    "lucide-react": "^0.244.0",
    "memorystore": "^1.6.7",
    
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.1",
    "react-remove-scroll": "^2.5.7",
    "recharts": "^2.7.2",
    "tailwind-merge": "^1.13.2",
    "tailwindcss-animate": "^1.0.6",
    "wouter": "^2.11.0",
    "ws": "^8.18.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "esbuild": "^0.18.11",
    "eslint": "^9.16.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "postcss": "^8.4.26",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.0",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.3.3",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vite": "^4.4.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "",
  "license": "MIT"
}
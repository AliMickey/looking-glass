{
  "name": "PhotoGlass",
  "version": "0.1.0",
  "description": "A modern network monitoring and diagnostic tool with an elegant UI for real-time network visualization",
  "private": true,
  "main": "dist/server/index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite --port 3000",
    "dev:server": "tsx watch server/index.ts",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:client": "tsc && vite build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outfile=dist/server/index.js",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "start": "NODE_ENV=production node dist/server/index.js",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "format": "prettier --write \"**/*.{ts,tsx,css,json,md}\"",
    "validate": "npm run typecheck && npm run lint && npm run test"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "1.1.2",
    "@radix-ui/react-alert-dialog": "1.0.5",
    "@radix-ui/react-aspect-ratio": "1.0.3",
    "@radix-ui/react-checkbox": "1.0.4",
    "@radix-ui/react-collapsible": "1.0.3",
    "@radix-ui/react-context-menu": "2.1.5",
    "@radix-ui/react-dialog": "1.0.5",
    "@radix-ui/react-dropdown-menu": "2.0.6",
    "@radix-ui/react-hover-card": "1.0.7",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-menubar": "1.0.4",
    "@radix-ui/react-navigation-menu": "1.1.4",
    "@radix-ui/react-progress": "1.0.3",
    "@radix-ui/react-radio-group": "1.1.3",
    "@radix-ui/react-scroll-area": "1.0.5",
    "@radix-ui/react-select": "1.2.2",
    "@radix-ui/react-separator": "1.0.3",
    "@radix-ui/react-slider": "1.1.2",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-switch": "1.0.3",
    "@radix-ui/react-tabs": "1.0.4",
    "@radix-ui/react-toast": "1.1.5",
    "@radix-ui/react-tooltip": "1.0.7",
    "@tanstack/react-query": "4.36.1",
    "class-variance-authority": "0.7.0",
    "clsx": "2.0.0",
    "cmdk": "0.2.0",
    "date-fns": "2.30.0",
    "express": "4.18.2",
    "express-session": "1.17.3",
    "framer-motion": "10.16.4",
    "lucide-react": "0.292.0",
    "react": "18.2.0",
    "react-day-picker": "8.9.1",
    "react-dom": "18.2.0",
    "react-hook-form": "7.48.2",
    "recharts": "2.9.3",
    "tailwind-merge": "2.0.0",
    "tailwindcss-animate": "1.0.7",
    "wouter": "2.12.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "20.10.0",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "@vitejs/plugin-react": "4.2.0",
    "autoprefixer": "10.4.16",
    "concurrently": "8.2.2",
    "esbuild": "0.19.8",
    "eslint": "8.54.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "postcss": "8.4.31",
    "prettier": "3.1.0",
    "rimraf": "5.0.5",
    "tailwindcss": "3.3.5",
    "tsx": "4.6.0",
    "typescript": "5.3.2",
    "vite": "5.0.0",
    "vitest": "0.34.6"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}